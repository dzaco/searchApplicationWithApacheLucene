	        hasError = true;
	}
 
        if(hasError == true) { return false; }
 
    });

 $("#uzas").keyup(function()
     {
          var max = 2000
          var box=$(this).val();
          var main = box.length *100;
          var value= (main / max);
          var count= max - box.length;

          if(box.length <= max)
          {
               $('#count').html(count);
               $('#bar').animate(
               {
                    "width": value+'%',
               }, 1);
          }
          else
          {
               $(this).val($(this).val().substring(0,max - 1));
          }
          return false;
     });
 $("#desc").keyup(function()
     {
          var max = 1500
          var box=$(this).val();
          var main = box.length *100;
          var value= (main / max);
          var count= max - box.length;

          if(box.length <= max)
          {
               $('#count1').html(count);
               $('#bar1').animate(
               {
                    "width": value+'%',
               }, 1);
          }
          else
          {
               $(this).val($(this).val().substring(0,max - 1));
          }
          return false;
     });

$('#same_person').change(function() {
	var checked = $('#same_person:checkbox:checked').val();
	if (checked == 1) {
		$('#zglaszajacy').attr('disabled','disabled');
	} else {
		$('#zglaszajacy').removeAttr('disabled');
	}

});

