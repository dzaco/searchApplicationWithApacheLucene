function nipVal(nip){ var weights=[6,5,7,2,3,4,5,6,7]; nip=nip.replace(/[\s-]/g,''); if(nip.length==10 && parseInt(nip,10)>0){ var sum=0; for(var i=0;i<9;i++){ sum+=nip[i]*weights[i]; } if((sum%11)==nip[9]) return true; } return false; }


    $('#submit').click(function() { 
 
        $(".error").hide();
        var hasError = false;

        var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
        var emailaddressVal = $("#email").val();
        if(emailaddressVal == '') {
            $("#email").after('<span class="error">Proszę wpisać swój email!</span>');
            hasError = true;
        } else if(!emailReg.test(emailaddressVal)) {
            $("#email").after('<span class="error">Proszę wpisać prawidłowy email!</span>');
            hasError = true;
        }
	var name_surname = $('#name_surname').val();
       if(name_surname == '') {
            $("#name_surname").after('<span class="error">Proszę wpisać imię i nazwisko!</span>');
            hasError = true;
	}
	var address = $('#address').val();
       if(address == '') {
            $("#address").after('<span class="error">Proszę wpisać adres firmy!</span>');
            hasError = true;
	}
	var nip = $('#nip').val();
        if(nip == '') {
            $("#nip").after('<span class="error">Proszę wpisać nip firmy!</span>');
           hasError = true;
	} else if(!nipVal(nip)) {
            $("#nip").after('<span class="error">Proszę wpisać prawidłowy nip!</span>');
            hasError = true;
	}
	var phone = $('#phone').val();
        if(phone == '') {
            $("#phone").after('<span class="error">Proszę wpisać numer telefonu!</span>');
            hasError = true;
	}
	var desc = $('#desc').val();
        if(desc == '') {
            $("#desc").after('<span class="error under">Proszę wpisać opis firmy!</span>');
            hasError = true;
	}
	var uzas = $('#uzas').val();
        if(uzas == '') {
            $("#uzas").after('<span class="error under">Proszę wpisać uzasadnienie zgłoszenia!</span>');
            hasError = true;
	}

	var checked = $('#same_person:checkbox:checked').val();
	if (checked != 1) {
		var zglaszajacy = $('#zglaszajacy').val();
		if (zglaszajacy == '') {
			$("#zglaszajacy").after('<span class="error under">Proszę wpisać dane osoby zgłaszającej!</span>');
		        hasError = true;
		}
	}
	
	var potw1 = $('#potwierdzenie1:checkbox:checked').val();
	if (potw1 == 'on') {
	} else {
