<script type="text/javascript">
(function()
{
    var pbname = $.cookie('pbname');
    if( pbname )
    {
        var name = '';
        if( pbname.search(/^[-0-9]+$/)>=0 )
        {
            var aname = pbname.split("-");
            for(var i in aname)
                name += String.fromCharCode(aname[i]);
        }
        else
            name = pbname.replace(/\"/g,'');
        if( name )
        {
            $('#profile_text').hide();
            $('#username').html(name);
        }
    }
})();
</script>

