<script type="text/javascript">
function DatePicker(prefix)
{
    this.cursor = 0;
    this.items = 4;
    this.prefix = prefix; 
    this.getCurrent = function()
    {
        return document.getElementById(this.prefix + this.cursor);
    };
    this.next = function()
    {
        this.step(1);
    };
    
    this.prev = function()
    {
        this.step(-1);
    };
    
    this.step = function(step)
    {
        this.getCurrent().style.display = 'none';
        this.cursor = (this.cursor + this.items + step) % this.items;
        this.getCurrent().style.display = 'block';
    };
}
var dpicker = new DatePicker('calendar_month_');

</script>

