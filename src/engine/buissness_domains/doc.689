  <script type="text/javascript">
function renderEmbed(dimensionInput, dimension) {
    if (dimensionInput.val() == "" || isNaN(dimensionInput.val()))
        return null;

    textArea = $("#video_embed .embed-form textarea");
    if (dimension == "width") {
        height = Math.round(dimensionInput.val() / 1.61);
        $(".embed-form #id_height").val(height);
        textArea.val(textArea.val().replace(/width="(\d+)"/gi, 'width="' + dimensionInput.val() + '"'));
        textArea.val(textArea.val().replace(/height="(\d+)"/gi, 'height="' + height + '"'));
    }	

    if (dimension == "height") {
        width = Math.round(dimensionInput.val() * 1.61);
        $(".embed-form #id_width").val(width);
        textArea.val(textArea.val().replace(/height="(\d+)"/gi, 'height="' + dimensionInput.val() + '"'));
        textArea.val(textArea.val().replace(/width="(\d+)"/gi, 'width="' + width + '"'));
    }
}

$(document).ready(function() {
    $(".embed a").click(function(e) {
        e.preventDefault();

        var embedMarTop = ($('#video_embed').height() + 80) / 2;
        var embedMarLeft = ($('#video_embed').width() + 80) / 2;

        $('#video_embed').css({
            'margin-top' : -embedMarTop,
            'margin-left' : -embedMarLeft
        }).show();

        $('body').append('<div id="fade"></div>');
        $('#fade').css({'filter' : 'alpha(opacity=80)'}).show();

        $("#fade,#video_embed .close_dialog").live("click", function() {
            $("#fade").remove();
            $("#video_embed").hide();
        });

        $("#video_embed .embed-form textarea").focus().select();
    });

    $("#video_embed .embed-form #id_width").keyup(function() {
        renderEmbed($(this), "width");
    });

    $("#video_embed .embed-form #id_height").keyup(function() {
        renderEmbed($(this), "height");
    });
});
</script>



